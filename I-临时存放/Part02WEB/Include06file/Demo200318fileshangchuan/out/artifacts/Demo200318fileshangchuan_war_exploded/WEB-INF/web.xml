<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>shangchuan.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/200318first</url-pattern>
    </servlet-mapping>

   <servlet>
        <servlet-name>UpploadServlet</servlet-name>
        <servlet-class>shangchuan.UpploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpploadServlet</servlet-name>
        <url-pattern>/200318second</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DowloadServlet</servlet-name>
        <servlet-class>Download.DowloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DowloadServlet</servlet-name>
        <url-pattern>/200318third</url-pattern>
    </servlet-mapping>
</web-app>
